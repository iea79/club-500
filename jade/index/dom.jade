                    .dom__slider__top
                        each item in domSlider
                            .dom__slider__block(style="background-image:url(img/dom/slider/"+item.img+")" 
                                )
                                .dom__slider__top__convert
                                    .dom__slider__title= item.title
                                    .dom__slider__sub= item.text
                                    .dom__slider__hrefs
                                        a(href="#", target="_blank").dom__slider_a.dom__slider__360
                                        a(href="#", target="_blank").dom__slider_a.dom__slider__eye


                    .dom__slider_map
                        img(src="img/dom/places.svg", alt="")

                    .dom__slider__bottom
                        each item in domSlider
                            .dom__slider__bottom__block
                                img.dom__slider__bottom_img(src="img/dom/slider/"+item.img+"", alt="")



